    "cryptiles": "^3.1.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^1.17.0",
    "hoek": "^4.1.0",
    "is-promise": "^2.1.0",
    "jade": "^1.11.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.2",
    "newrelic": "^1.35.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-fastclick": "^2.1.2",
    "react-router": "^1.0.3",
    "react-tap-event-plugin": "^2.0.1",
    "readable-stream": "^2.2.2",
    "request": "^2.79.0",
