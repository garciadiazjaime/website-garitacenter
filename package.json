    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "1.0.0",
    "gulp-if": "^2.0.2",
    "gulp-minify": "0.0.14",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^2.0.0",
    "install": "^0.8.2",
    "node-sass": "^4.0.0",
    "nodemon": "^1.11.0",
    "npm": "^4.0.5",
    "prettydiff": "^99.0.1",
    "react-hot-loader": "^1.3.1",
    "run-sequence": "^1.2.2",
    "sass-loader": "^4.1.0",
    "sprity": "^1.0.8",
    "sprity-js": "^1.1.1",
    "sprity-sass": "^1.0.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
